{
	"compilerOptions": {
		"target": "es5",
		"outDir": "./dist",
		"noImplicitAny": false,
		"strict": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"strictNullChecks": false,
		"typeRoots": [
			"../node_modules/@types"
		],
	},
	"exclude": [
		"node_modules/",
		"test"
	],
	"include": [
		"lib",
		"./app.ts",
		"views"
	]
}